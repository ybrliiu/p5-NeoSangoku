<!-- コマンド一覧 -->
<div id="wrap-command" class="scroll">
  <table id="command">
    <tr>
      <th colspan="3">コマンド入力</th>
    </tr>
    % for (0 .. @$command - 1) {
    <tr id="<%= $_ %>">
      <td>
        <input type="checkbox" name="no" value="<%= $_ %>"><%= $_ + 1 %>
      </td>
      <td>
        <%= $command->[$_]->id %></td><td><%= $command->[$_]->detail %>
      </td>
    </tr>
    % }
  </table>
</div>
